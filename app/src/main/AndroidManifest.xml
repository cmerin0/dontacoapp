<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dontaco">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DonTaco">
        <activity android:name=".Register"></activity>
        <activity android:name=".Cart" />
        <activity android:name=".ShowOrders" />
        <activity android:name=".CreateProduct" />
        <activity android:name=".AddProductCart" />
        <activity android:name=".CreateOrder"></activity>
        <activity android:name=".ShowProducts" />
        <activity android:name=".Login"></activity>
        <activity android:name=".IndexActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@android:style/Theme.DeviceDefault.NoActionBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>